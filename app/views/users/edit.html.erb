<h1>Edit Profile Pic</h1>
<%=form_for @user do |f| %>
<%=f.hidden_field :username %> <br>
<%=f.hidden_field :email %> <br>
<%= f.label :user_pic %>
<div class="placeformat">
    <%= f.text_field :user_pic, value: 'Drop in an image url to customize your profile!' %>
</div>
<%=f.hidden_field :password %> <br>
<%=f.hidden_field :password_confirmation %> <br>
<%=f.submit %>
<%end %>
