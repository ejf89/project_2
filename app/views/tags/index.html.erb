<div class="container">
<div class="row"></div>
<div class="row"></div>
<center><i class="small material-icons">star star star star star star star star star star</i><h4>Most Popular Tags</h4><i class="small material-icons">star star star star star star star star star star</i></center>
<div class="row"></div>
<div class="row"></div>
<div class="row">
    <% Tag.most_popular.each do |tag| %>
        <div class="col s8 m4">
          <div class="card">
              <% if tag.pictures.last != nil %>

                    <div class="card-image">
                      <img src="<%=tag.pictures.last.image_url%>">
                    </div>
                  <div class="card-action">
                    <%= link_to tag.name, tag_path(tag) %>
                  </div>
                  <% end %>
          </div>
      </div>

    <% end %>
    </div>

    <div class="row"></div>
    <div class="row"></div>
  <center><i class="small material-icons">star star star star star star star star star star</i><h4>Trending Tags</h4><i class="small material-icons">star star star star star star star star star star</i></center>
    <div class="row"></div>
    <div class="row">
        <% Tag.trending.each do |tag| %>
            <div class="col s8 m4">
              <div class="card">
                <div class="card-image">
                  <img src="<%=tag.pictures.last.image_url%>">
                </div>
                  <div class="card-action">
                    <%= link_to tag.name, tag_path(tag) %>
                  </div>
              </div>
          </div>

        <% end %>
        </div>




</div>
